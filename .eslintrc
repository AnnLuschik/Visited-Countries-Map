{
  "root": true,
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "react-app", 
    "react-app/jest",
    "airbnb"
  ],
  "plugins": ["react"],
  "rules": {
    "import/prefer-default-export": 0,
    "react/jsx-props-no-spreading": 0,
    "react/button-has-type": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "no-plusplus": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "react/require-default-props": 0
  }
}